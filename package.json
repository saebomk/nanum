{
  "name": "notube",
  "version": "1.0.0",
  "description": "Youtube clone app with nodejs",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:server": "nodemon --exec babel-node src/init.js --delay 2 --ignore '.scss' --ignore 'static' ",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server": "babel src --out-dir build --ignore 'src/assets','src/static','src/webpack.config.js'",
    "copyAll": "cp -R src/static build && cp -R src/views build",
    "build": "npm run build:server && npm run build:assets && npm run copyAll",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000",
    "start": "PRODUCTION=true node build/init.js",
    "prestart": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/saebomk/notube.git"
  },
  "author": "Saebom Kwon",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/saebomk/notube/issues"
  },
  "homepage": "https://github.com/saebomk/notube#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/cli": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/node": "^7.12.10",
    "@babel/polyfill": "^7.12.1",
    "autoprefixer": "^10.1.0",
    "aws-sdk": "^2.817.0",
    "axios": "^0.21.1",
    "babel-loader": "^8.2.2",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.8.1",
    "css-loader": "^5.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.2",
    "helmet": "^4.2.0",
    "mongoose": "^5.11.8",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "passport": "^0.4.1",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^4.1.0",
    "pug": "^3.0.0",
    "sass-loader": "^10.1.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  },
  "devDependencies": {
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "mini-css-extract-plugin": "^1.3.3",
    "nodemon": "^2.0.6",
    "postcss": "^8.2.1",
    "prettier": "^2.2.1",
    "sass": "^1.30.0"
  }
}
